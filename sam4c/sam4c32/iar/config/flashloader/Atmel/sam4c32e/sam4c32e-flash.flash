<?xml version="1.0" encoding="iso-8859-1"?>

<flash_device>
  <exe>$TOOLKIT_DIR$\config\flashloader\Atmel\sam4c32e\sam4c32e-flash.out</exe>
  <flash_base>0x01000000</flash_base>
  <page>2048</page>
  <block>128 0x2000</block>
  <macro>$TOOLKIT_DIR$\config\debugger\Atmel\Trace_SAM4C32.dmac</macro>
  <args_doc>[--flash] specifies the flash loader to program the internal flash.
[--boot] is given, device will boot from flash after programming (GPNVM bit set). 
</args_doc>
</flash_device>
